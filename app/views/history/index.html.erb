<div class="container">
  <div class="jumbotron">
    <h1>Search History</h1>
    <% @historys.each do |history| %>
      <a href="/history/index">
        <ul class="list-group">
          <li class="list-group-item list-group-item-light">
            <%= history[:query] %>
          </li>
        </ul>
      </a>
    <% end %>
  </div>
</div>
